<mapper
	namespace="ltd.newbee.mall.newbeemall.dao.GoodsImageMapper">
	<select id="goodsimage"
		resultType="ltd.newbee.mall.newbeemall.entity.GoodsImage">
		SELECT * FROM newbee_mall_db.goods_image where goods_id=#{goodsId} order by goods_rank;
	</select>

</mapper>


